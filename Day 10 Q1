#include <stdio.h>
#include <string.h>

int main() {
    char str[100];
    int left = 0;

    printf("Enter string: ");
    scanf("%s", str);

    int right = strlen(str) - 1;
    int isPalindrome = 1;

    while (left < right) {
        if (str[left] != str[right]) {
            isPalindrome = 0;
            break;
        }
        left++;
        right--;
    }

    if (isPalindrome)
        printf("Palindrome");
    else
        printf("Not Palindrome");

    return 0;
}
